<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DStarInterviewNotes" parent="Theme.MaterialComponents.DayNight">
        <!-- Primary brand color. -->
        <item name="actionModeBackground">@color/light_peach</item>
        <item name="colorPrimary">@color/light_peach</item>
        <item name="statusBarBackground">#00120B</item>
        <item name="colorPrimaryVariant">@color/light_peach</item>
        <item name="colorOnPrimary">#00120B</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#26547C</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="AlertDialogCustom" parent="Theme.AppCompat.DayNight.Dialog.Alert">
        <item name="android:textColor">@color/primary_note_title</item>
    </style>

    <color name="primary_background">@color/light_peach</color>
    <color name="primary_note">#FFFFFF</color>
    <color name="primary_note_title">#000000</color>
    <color name="primary_note_text">#000000</color>

    <style name="TextStyle">
        <item name="android:background">?attr/colorSurface</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>



</resources>